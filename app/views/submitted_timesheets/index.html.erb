<p id="notice"><%= notice %></p>

<h1>Listing Submitted Timesheets</h1>

<table>
  <thead>
    <tr>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @submitted_timesheets.each do |submitted_timesheet| %>
      <tr>
        <td><%= link_to 'Show', submitted_timesheet %></td>
        <td><%= link_to 'Edit', edit_submitted_timesheet_path(submitted_timesheet) %></td>
        <td><%= link_to 'Destroy', submitted_timesheet, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Submitted timesheet', new_submitted_timesheet_path %>
