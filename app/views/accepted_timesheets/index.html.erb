<p id="notice"><%= notice %></p>

<h1>Listing Accepted Timesheets</h1>

<table>
  <thead>
    <tr>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @accepted_timesheets.each do |accepted_timesheet| %>
      <tr>
        <td><%= link_to 'Show', accepted_timesheet %></td>
        <td><%= link_to 'Edit', edit_accepted_timesheet_path(accepted_timesheet) %></td>
        <td><%= link_to 'Destroy', accepted_timesheet, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Accepted timesheet', new_accepted_timesheet_path %>
